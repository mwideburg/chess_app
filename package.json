    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "repository": {
